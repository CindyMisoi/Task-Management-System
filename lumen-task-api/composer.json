{
    "name": "laravel/lumen-task-api",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Laravel\\LumenTaskApi\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "CindyMisoi",
            "email": "crmisoi2k@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/lumen": "^8.3|^9.2",
        "illuminate/database": "^8.83|^9.22"
    },
    "require-dev": {
        "laravel/tinker": "^2.9"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}